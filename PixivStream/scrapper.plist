<dict>
	<key>encodings</key>
	<string>utf-8</string>
	<key>url</key>
	<dict>
		<key>new</key>
		<string>http://www.pixiv.net/new_illust.php?p=%d</string>
		<key>search</key>
		<string>http://www.pixiv.net/search.php?s_mode=s_tag&amp;word=%@&amp;p=%d</string>
